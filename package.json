{
  "name": "qurriqulum",
  "version": "2.0.0",
  "year": "2024",
  "description": "A resume/game by Giuseppe De Ponte, fullstack JS web dev",
  "main": "script/main.js",
  "devDependencies": {
    "eslint": "^9.9.1",
    "eslint-config-google": "^0.9.1"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "env": {
      "browser": true
    },
    "parserOptions": {
      "ecmaVersion": 5
    },
    "globals": {
      "CONFIG": false,
      "HELPERS": false
    },
    "rules": {
      "eqeqeq": 2,
      "semi": 2,
      "no-var": 0,
      "quotes": [
        "error",
        "single"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "max-len": 0
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/giuseppedeponte/qurriqulum"
  },
  "keywords": [
    "game",
    "resume",
    "web",
    "developer"
  ],
  "author": "Giuseppe De Ponte",
  "license": "ISC"
}